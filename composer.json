{
  "name": "binaryversion/laravel-gmail",
  "description": "Gmail API package for Laravel, branch from dacastro4/laravel-gmail.",
  "keywords": [
    "api",
    "laravel",
    "Gmail"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jo√£o Martins",
      "email": "joao.martins@binaryversion.pt",
      "homepage": "https://binaryversion.pt"
    }
  ],
  "require": {
    "php": "^7.3",
    "google/apiclient": "^2.8",
    "illuminate/auth": "~5.8|^6.0|^7.0|^8.0",
    "illuminate/config": "~5.8|^6.0|^7.0|^8.0",
    "illuminate/database": "~5.8|^6.0|^7.0|^8.0",
    "illuminate/routing": "~5.8|^6.0|^7.0|^8.0",
    "illuminate/session": "~5.8|^6.0|^7.0|^8.0",
    "illuminate/support": "~5.8|^6.0|^7.0|^8.0",
    "swiftmailer/swiftmailer": "~5.8|^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "mockery/mockery": "^1.3.1",
    "squizlabs/php_codesniffer": "~3.5",
    "orchestra/testbench": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Binaryversion\\LaravelGmail\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Binaryversion\\LaravelGmail\\LaravelGmailServiceProvider"
      ],
      "aliases": {
        "LaravelGmail": "Binaryversion\\LaravelGmail\\Facade\\LaravelGmail"
      }
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
